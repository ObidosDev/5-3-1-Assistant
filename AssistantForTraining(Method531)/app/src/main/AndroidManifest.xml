<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dev.obidos.wrd.assistantfortrainingmethod531"
    android:versionCode="8"
    android:versionName="1.3.1">

    <uses-permission android:name="android.permission.VIBRATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@drawable/icon"
        android:theme="@style/AppTheme"
        android:name="dev.obidos.wrd.assistantfortrainingmethod531.AssistantApp">

        <activity android:name="dev.obidos.wrd.assistantfortrainingmethod531.activity.SplashActivity"
            android:screenOrientation="sensorPortrait">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER"/>
                <action android:name="android.intent.action.MAIN"/>
            </intent-filter>
        </activity>

        <activity android:name="dev.obidos.wrd.assistantfortrainingmethod531.activity.ExercisesActivity"
            android:screenOrientation="sensorPortrait">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

        <activity android:name="dev.obidos.wrd.assistantfortrainingmethod531.activity.WeightChartActivity"
            android:screenOrientation="sensorPortrait">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

        <activity android:name="dev.obidos.wrd.assistantfortrainingmethod531.activity.AddExerciseActivity"
            android:screenOrientation="sensorPortrait"
            android:windowSoftInputMode="adjustResize">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

        <activity android:name="dev.obidos.wrd.assistantfortrainingmethod531.activity.ExerciseInfoActivity"
            android:screenOrientation="sensorPortrait">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

        <activity android:name="dev.obidos.wrd.assistantfortrainingmethod531.activity.SettingsActivity"
            android:screenOrientation="sensorPortrait">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

        <activity android:name="dev.obidos.wrd.assistantfortrainingmethod531.activity.AboutActivity"
            android:screenOrientation="sensorPortrait">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

        <activity android:name="dev.obidos.wrd.assistantfortrainingmethod531.activity.LastSetCalculationActivity"
            android:screenOrientation="sensorPortrait">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

        <activity android:name="dev.obidos.wrd.assistantfortrainingmethod531.activity.WeightExerciseChartActivity"
            android:screenOrientation="sensorPortrait">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

        <activity android:name="dev.obidos.wrd.assistantfortrainingmethod531.activity.SyncActivity"
            android:screenOrientation="sensorPortrait">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

        <activity
            android:name="com.dropbox.client2.android.AuthActivity"
            android:launchMode="singleTask"
            android:theme="@android:style/Theme.Translucent.NoTitleBar"
            android:configChanges="orientation|keyboard">
            <intent-filter>
                <data android:scheme="db-cjm7592hf6uq183" />
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.BROWSABLE"/>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

    </application>

</manifest>
